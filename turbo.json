{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["evmos-wallet#build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "NEXT_PUBLIC_GOOGLE_ANALYTICS_ID"
      ]
    },
    "eslint-config-custom#build": {},
    "tailwind-config#build": {},
    "tsconfig#build": {},
    "jest-config-evmos#build": {},
    "constants-helper#build": {},
    "icons#build": {},
    "helpers#build": {},
    "services#build": {},
    "ui-helpers#build": {
      "dependsOn": ["icons#build", "constants-helper#build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "evmos-wallet#build": {
      "dependsOn": ["icons#build", "ui-helpers#build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "eslint-config-custom#dev": {},
    "tailwind-config#dev": {},
    "tsconfig#dev": {},
    "constants-helper#dev": {},
    "icons#dev": {},
    "helpers#dev": {},
    "services#dev": {},
    "ui-helpers#dev": {
      "dependsOn": ["icons#dev", "constants-helper#dev"]
    },
    "evmos-wallet#dev": {
      "dependsOn": ["icons#dev", "ui-helpers#dev"]
    },
    "lint": {},
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "test": {},
    "test:watch": {
      "cache": false
    },
    "test:coverage": {
      "cache": false
    }
  }
}